      <LeatherAmount>25</LeatherAmount>
      <PsychicSensitivity>0.85</PsychicSensitivity>
      <MiningSpeed>1.2</MiningSpeed>
    </statBases>
    <tools>
      <li>
        <label>left fist</label>
